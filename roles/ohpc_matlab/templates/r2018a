#%Module

set ver r2018a
set matlabroot /opt/ohpc/pub-master/apps/matlab/M2/
set url http://docs.uabgrid.uab.edu/wiki/MatLab

proc ModulesHelp { } {
   puts stderr $msg
}

module-whatis   $msg

#module load foss/2016b

setenv MATLAB $matlabroot
setenv MATLABROOT $matlabroot
setenv MATLAB_HOME $matlabroot
prepend-path PATH $matlabroot/bin
